{
  "name": "@raikuxq/alg-ds",
  "scripts": {
    "test": "jest",
    "dev": "nodemon --exec tsx src/index.ts",
    "build:lib": "rimraf ./lib && vite build",
    "build:dts": "dts-bundle-generator -o lib/index.d.ts src/index.ts --no-check --inline-declare-externals false --external-inlines @babel/types",
    "build:full": "yarn build:lib && yarn build:dts",
    "start": "npm run build && node lib/index.js",
    "lint": "yarn eslint . --ext .ts",
    "lint:fix": "yarn eslint --fix .  --ext .ts",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raikuxq/lab-ts-algorithms"
  },
  "keywords": [
    "algorithms",
    "data structures",
    "graph",
    "undirected graph",
    "directed graph",
    "breadth-first",
    "depth-first",
    "BFS",
    "DFS",
    "shortest path",
    "binary search tree",
    "randomized binary search tree",
    "BST",
    "dijkstra",
    "quick sort",
    "merge sort",
    "binary search",
    "linked list",
    "doubly linked list",
    "singly linked list",
    "stack",
    "queue"
  ],
  "main": "./lib/index.mjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vuepress/plugin-search": "^2.0.0-beta.48",
    "@vuepress/theme-default": "^2.0.0-beta.48",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.0",
    "rimraf": "^6.1.2",
    "terser": "^5.46.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-esm": "^0.0.6",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vuepress": "^2.0.0-beta.48"
  },
  "dependencies": {},
  "version": "2.0.0",
  "sideEffects": false
}
